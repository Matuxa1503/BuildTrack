{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.mjs"
    },
    {
      "src": "/last",
      "dest": "/api/routes/last.mjs"
    },
    {
      "src": "/addUser",
      "dest": "/api/routes/addUser.mjs"
    },
    {
      "src": "/itemUser",
      "dest": "/api/routes/itemUser.mjs"
    },
    {
      "src": "/deleteUser",
      "dest": "/api/routes/deleteUser.mjs"
    }
  ]
}
