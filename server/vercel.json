{
  "version": 2,
  "functions": {
    "tg-bot/botWebhook.mjs": { "runtime": "nodejs18.x" }
  },
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.mjs"
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "0 9 * * *"
    }
  ]
}
